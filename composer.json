{
    "name": "adobe/target-php-sdk",
    "description": "Adobe Target PHP SDK",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "autoload": {
        "psr-4": {
            "Adobe\\Target\\Edge\\Client\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^2",
        "phpstan/phpstan": "^0.12.56"
    },
    "require": {
        "jwadhams/json-logic-php": "^1"
    },
    "scripts": {
        "generate-client": "cd codegeneration && npm run codegen",
        "codegen": "composer generate-client",
        "test": "phpunit tests",
        "format": "php-cs-fixer fix",
        "analyze": "phpstan analyse src tests"
    }
}
